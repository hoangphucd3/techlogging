<h1>Articles</h1>

<h2>List</h2>
<div class="table-responsive">
  <table class="table table-striped">
    <thead>
    <tr>
      <th>#</th>
      <th>Title</th>
      <th></th>
    </tr>
    </thead>
    <tbody>
    <% @articles.each do |article| %>
    <tr>
      <td><%= article.id %></td>
      <td><%= article.title %></td>
      <td></td>
    </tr>
    <% end %>
    </tbody>
  </table>
</div>
